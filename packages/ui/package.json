{
  "name": "@nerve/ui",
  "version": "1.0.0",
  "description": "A modern UI library for The Nerve",
  "private": false,
  "scripts": {
    "check:lint": "TIMING=1 eslint . --no-error-on-unmatched-pattern",
    "fix:lint": "eslint .",
    "test": "echo \"Bypassing: no test specified\" && exit 0"
  },
  "dependencies": {
    "@nerve/icons": "workspace:*",
    "@nerve/kit": "workspace:*",
    "@nerve/tokens": "workspace:*",
    "@nerve/tsconfig": "workspace:*",
    "@radix-ui/react-slot": "^1.0.2",
    "cva": "npm:class-variance-authority@0.6.1",
    "next": "^14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@nerve/tsconfig": "workspace:*",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.14",
    "cssnano": "6.0.1",
    "eslint-config-nerve": "workspace:*",
    "postcss": "8.4.25",
    "tailwindcss": "3.3.2",
    "typescript": "5.4.5"
  },
  "exports": {
    ".": "./src/components/index.ts",
    "./package.json": "./package.json",
    "./src/components/*": "./src/components/*",
    "./postcss": "./postcss.config.cjs",
    "./tailwind": "./tailwind.config.cjs",
    "./design-system.css": "./css/design-system.css"
  },
  "typesVersions": {
    "*": {
      "index": [
        "./src/components/index.ts"
      ],
      "postcss": [
        "./postcss.config.cjs"
      ],
      "tailwind": [
        "./tailwind.config.cjs"
      ],
      "src/components/*": [
        "./src/components/*"
      ],
      "design-system.css": [
        "./css/design-system.css"
      ]
    }
  }
}
