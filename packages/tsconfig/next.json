{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next",
  "extends": "./base.json",
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2017",
    "module": "esnext",
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "incremental": true,
    "noEmit": true,
    "isolatedModules": true
  },
  "exclude": ["node_modules"]
}
