{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next",
  "extends": "./react.json",
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "incremental": true,
    "noEmit": true,
    "module": "esnext",
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "exclude": ["node_modules"]
}
