{
  "name": "@nerve/kit",
  "version": "1.0.0",
  "description": "A re-useable code-kit for The Nerve",
  "private": false,
  "scripts": {
    "build": "tsup --minify",
    "dev:pre-build": "tsup --silent",
    "check:lint": "TIMING=1 eslint . --no-error-on-unmatched-pattern",
    "fix:lint": "eslint . --fix",
    "test": "echo \"Bypassing: no test specified\" && exit 0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@nerve/tsconfig": "workspace:*",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "eslint-config-nerve": "workspace:*",
    "tsup": "7.1.0",
    "typescript": "5.4.5"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    "./env-vars": {
      "import": "./src/env-vars/index.ts",
      "require": "./dist/env-vars.js"
    },
    "./node": {
      "import": "./src/node/index.ts",
      "require": "./dist/node.js"
    },
    "./utils": {
      "import": "./src/utils/index.ts",
      "require": "./dist/utils.js"
    }
  },
  "typesVersions": {
    "*": {
      "env-vars": [
        "src/env-vars/index.ts",
        "dist/env-vars.d.ts"
      ],
      "node": [
        "src/node/index.ts",
        "dist/node.d.ts"
      ],
      "utils": [
        "src/utils/index.ts",
        "dist/utils.d.ts"
      ]
    }
  }
}
