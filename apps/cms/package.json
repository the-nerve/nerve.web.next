{
  "name": "@nerve/cms",
  "version": "1.0.0",
  "description": "A Next-gen CMS for The Nerve Theatre web app",
  "private": true,
  "main": "package.json",
  "scripts": {
    "check:lint": "TIMING=1 eslint .",
    "check:types": "tsc --noEmit",
    "dev": "sanity dev",
    "start": "sanity start",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "deploy-graphql": "sanity graphql deploy",
    "fix:lint": "eslint .",
    "sync:env": "vercel env pull .env.local",
    "test": "echo \"Bypassing: no test specified\" && exit 0",
    "vercel:link": "vercel --scope the-nerve link --project cms --yes"
  },
  "keywords": [
    "sanity"
  ],
  "dependencies": {
    "@radix-ui/react-icons": "1.3.0",
    "@sanity/vision": "^3.39.1",
    "date-fns": "3.6.0",
    "just-kebab-case": "4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "2.0.10",
    "sanity": "^3.39.1",
    "sanity-plugin-media": "2.2.5",
    "styled-components": "^6.1.8",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@nerve/tsconfig": "workspace:*",
    "@sanity/eslint-config-studio": "^4.0.0",
    "@types/react": "^18.0.25",
    "eslint-config-nerve": "workspace:*",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6"
  }
}
