{
  "name": "@nerve/web",
  "version": "1.0.0",
  "description": "Frontend for the next-gen Nerve web app",
  "private": false,
  "scripts": {
    "build": "next build",
    "check:lint": "TIMING=1 eslint .",
    "check:types": "tsc --noEmit",
    "dev": "next dev",
    "dev:sync": "pnpm vercel:sync; pnpm dev",
    "fix:lint": "eslint .",
    "start": "next start",
    "sync:env": "vercel env pull .env.local",
    "test": "echo \"Bypassing: no test specified\" && exit 0",
    "vercel:link": "vercel --scope the-nerve link --project web --yes"
  },
  "dependencies": {
    "@nerve/icons": "workspace:*",
    "@nerve/kit": "workspace:*",
    "@nerve/tailwind": "workspace:*",
    "@nerve/ui": "workspace:*",
    "@types/node": "20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@vercel/edge-config": "^0.2.1",
    "autoprefixer": "^10.4.14",
    "jotai": "^2.1.1",
    "next": "13.4.4",
    "next-sanity": "^4.3.3",
    "postcss": "^8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.44.3",
    "react-query": "^3.39.3",
    "tailwindcss": "^3.3.2",
    "typescript": "5.1.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@nerve/tsconfig": "workspace:*",
    "@svgr/webpack": "^8.0.1",
    "eslint-config-nerve": "workspace:*"
  }
}
