{
  "name": "@nerve/web",
  "version": "1.0.0",
  "description": "Frontend for the next-gen Nerve web app",
  "private": false,
  "scripts": {
    "build": "next build",
    "check:lint": "TIMING=1 eslint .",
    "check:types": "tsc --noEmit",
    "dev": "next dev",
    "dev:sync": "pnpm vercel:sync; pnpm dev",
    "fix:lint": "eslint .",
    "start": "next start",
    "sync:env": "vercel env pull .env.local",
    "test": "echo \"Bypassing: no test specified\" && exit 0",
    "vercel:link": "vercel --scope the-nerve link --project web --yes"
  },
  "dependencies": {
    "@nerve/icons": "workspace:*",
    "@nerve/kit": "workspace:*",
    "@nerve/tokens": "workspace:*",
    "@nerve/ui": "workspace:*",
    "@portabletext/react": "3.0.18",
    "@sanity/image-url": "^1.0.2",
    "@vercel/edge-config": "^0.2.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "next": "14.2.3",
    "next-sanity": "9.0.12",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "schema-dts": "^1.1.2",
    "server-only": "^0.0.1",
    "zod": "3.23.4"
  },
  "devDependencies": {
    "@nerve/tsconfig": "workspace:*",
    "@svgr/webpack": "^8.0.1",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "^10.4.14",
    "eslint-config-nerve": "workspace:*",
    "postcss": "^8.4.25",
    "tailwindcss": "^3.3.2",
    "typescript": "5.4.5"
  }
}
